set spoolfile = "+gsi/INBOX"
unset from;
set from="Joel Silva <j.silva@gsi.de>"
set sendmail="msmtp -a gsi"
set sendmail_wait=0
unset record
set record="+gsi/Sent\ Items"
set trash="+gsi/Deleted\ Items"
set mbox="+gsi/All Mail"
set postponed="=+gsi/Drafts"
unset signature;
set signature=""
color status red default
